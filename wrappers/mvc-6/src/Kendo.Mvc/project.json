{
  "version": "1.0.0-*",
  "name": "Kendo.Mvc",
  "title": "Telerik UI for ASP.NET MVC Core 1.0.0",
  "description": "Telerik UI for ASP.NET MVC provides server-side helpers for Kendo UI's JavaScript framework. It enables developers comfortable with the ASP.NET MVC framework to easily configure and work with Kendo UI via familiar server-side programming instead of relying solely on hand-coding of HTML and JavaScript.",
  "authors": [ "Telerik" ],
  "packOptions": {
    "owners": [ "Telerik" ],
    "licenseUrl": "http://www.telerik.com/purchase/license-agreement/aspnet-mvc",
    "projectUrl": "http://www.telerik.com/aspnet-mvc",
    "tags": [ "aspnet", "mvc", "aspnext", "vnext", "VS2015", "Kendo", "Kendo UI", "JavaScript", "UI" ],
    "requireLicenseAcceptance": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.Mvc": "1.0.0",
    "Microsoft.AspNetCore.Mvc.Core": "1.0.0",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.0.0",
    "Microsoft.AspNetCore.Mvc.Formatters.Xml": "1.0.0",
    "Microsoft.AspNetCore.Routing": "1.0.0",
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0",
    "System.Linq.Queryable": "4.0.1",
    "System.Data.Common": "4.1.0"
  },
  "buildOptions": {
    "warningsAsErrors": false,
    "xmlDoc": false,
    "embed": {
      "include": [ "*.resources" ]
    }
  },
  "frameworks": {
    "net451": { },
    "netstandard1.6": {
      "dependencies": {
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Xml.XmlDocument": "4.0.1"
      },
      "imports": [
        "dnxcore50"
      ]
    }
  }
}
